#!/sw/bin/perl5.8.6

eval 'exec /sw/bin/perl5.8.6  -S $0 ${1+"$@"}'
    if 0; # not running under some shell
use warnings;
use strict;
use File::Basename qw(dirname); 

BEGIN {
    eval {
        require Jifty::Util;
        my $root = Jifty::Util->app_root;
        unshift @INC, "$root/lib" if ($root);
    }

}

use Jifty::Script;
Jifty::Script->dispatch();
